<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="loadPane" alignment="CENTER" disable="false" fillWidth="true" prefHeight="250.0" prefWidth="250.0" visible="true" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="LoadController">
  <children>
    <ListView fx:id="listView" prefHeight="250.0" prefWidth="200.0">
      <VBox.margin>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x1" />
      </VBox.margin>
    </ListView>
    <Button fx:id="loadButton" mnemonicParsing="false" prefWidth="250.0" text="Load Character" VBox.margin="$x1" />
  </children>
</VBox>
